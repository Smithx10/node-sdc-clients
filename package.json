{
  "author": "Mark Cavage <mark.cavage@joyent.com>",
  "name": "sdc-clients",
  "description": "Contains node.js client libraries for SDC REST APIs.",
  "version": "6.1.0",
  "homepage": "http://www.joyent.com",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@git.joyent.com:node-sdc-clients.git"
  },
  "main": "lib/index.js",
  "dependencies": {
    "http-signature": "0.9.6",
    "ldapjs": "0.3.4",
    "lru-cache": "1.0.4",
    "node-uuid": "1.2.0",
    "nopt": "1.0.6",
    "restify": "0.4.4",
    "sprintf": "0.1.1",
    "smartdc": "6.5.0-1"
  },
  "devDependencies": {
    "whiskey": "0.6.1"
  },
  "scripts": {
    "pretest": "which gjslint; if [[ \"$?\" = 0 ]] ; then  gjslint --nojsdoc -e lib/node-smartdc/node_modules -r lib; else echo \"Missing gjslint. Skipping lint\"; fi",
    "test": "NO_PROVISION_TESTS=1 whiskey --quiet --timeout 120000 --sequential -t \"`find tst -name *.test.js | sort -r | xargs`\""
  },
  "engines": {
    "node": "~0.4.9"
  }
