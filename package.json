{
  "name": "sdc-clients",
  "description": "Contains node.js client libraries for SDC REST APIs.",
  "version": "7.0.3",
  "homepage": "http://www.joyent.com",
  "private": true,
  "main": "./lib/index.js",
  "dependencies": {
    "http-signature": "0.9.8",
    "ldapjs": "0.4.8",
    "lru-cache": "1.0.5",
    "node-uuid": "1.3.3",
    "sprintf": "0.1.1",
    "restify": "1.0.0",
    "node-uuid": "1.3.3"
  },
  "devDependencies": {
    "whiskey": "0.6.6"
  },
  "scripts": {
    "pretest": "which gjslint; if [[ \"$?\" = 0 ]] ; then  gjslint --nojsdoc -r lib; else echo \"Missing gjslint. Skipping lint\"; fi",
    "test": "NO_PROVISION_TESTS=1 ./node_modules/.bin/whiskey --quiet --timeout 120000 --sequential -t \"`find test -name *.test.js | sort -r | xargs`\""
  }
