    "lru-cache": "1.1.0",
    "node-uuid": "1.3.3",
    "sprintf": "0.1.1"
  },
  "devDependencies": {
    "bunyan": "0.6.8",
    "whiskey": "0.6.6"
  },
  "scripts": {
    "pretest": "which gjslint; if [[ \"$?\" = 0 ]] ; then  gjslint --nojsdoc -r lib; else echo \"Missing gjslint. Skipping lint\"; fi",
